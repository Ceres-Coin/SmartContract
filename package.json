{
  "name": "ceres",
  "version": "0.1.210628a",
  "description": "ceres",
  "main": "truffle-config.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "truffle test",
    "migration_clean": "rm -rf build & truffle compile --all",
    "migration_bscTestnet_reset": "truffle migrate --network testnet --skip-dry-run --reset",
    "migration_bscTestnet_noReset": "truffle migrate --network testnet --skip-dry-run",
    "migration_ropsten_reset": "truffle migrate --network ropsten --skip-dry-run --reset",
    "migration_ropsten_noReset": "truffle migrate --network ropsten --skip-dry-run",
    "clean_build": "rm -rf build & truffle compile --all & truffle migrate --network development --skip-dry-run --reset",
    "clean_build_singleFile" : "rm -rf build & truffle compile --all & truffle migrate -f 2 --to 2 --network development --skip-dry-run --reset",
    "truffleTestALL": "truffle test",
    "truffleTestSingleP2": "truffle test ./test/test_CERES_USDC_Pool_P2.js",
    "truffleTestSingleP1": "truffle test ./test/test_CERES_USDC_Pool_P1.js",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "dependencies": {
    "@openzeppelin/contracts": "^3.2.0",
    "@openzeppelin/test-helpers": "^0.5.11",
    "@truffle/hdwallet-provider": "^1.4.0",
    "big-number": "^2.0.0",
    "chai": "^4.3.4"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "devDependencies": {
    "@nomiclabs/hardhat-truffle5": "^2.0.0",
    "@nomiclabs/hardhat-web3": "^2.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "hardhat": "^2.3.0",
    "web3": "^1.3.6"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
